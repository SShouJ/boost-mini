/* feedback/pages/feedBack.wxss */
.proposal{
  background-color: white;
  border-radius: 15rpx;
  padding: 40rpx;
}
.textarea{
  height: 180rpx;
  width: 100%;
}
.camera{
  background-color: #eee;
  width: 250rpx;
  height: 250rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 15rpx;
  flex-direction: column;
  font-size: 30rpx;
}
.camera image{
  width: 70rpx;
  height: 70rpx;
  filter: invert(100%)
}
